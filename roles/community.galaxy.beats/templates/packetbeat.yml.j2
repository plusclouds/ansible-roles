############################# Packetbeat ######################################
packetbeat:
  interfaces:
    device: any
  {% for i in packetbeat_settings %}{% for a in i %}
"{{a}}":{{' '}}{{i[a]|to_json}}
  {% endfor %}{% endfor %}

output:
  logstash:  
    {{beat_logstash_output | to_json }}  
logging:  
  files:
    rotateeverybytes: 10485760 # = 10MB    
    keepfiles: 7 
